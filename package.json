{
  "name": "reason-cli",
  "version": "1.7.3",
  "preferGlobal": true,
  "description": "Globally Installed CLI Tools For ReasonML",
  "main": "index.js",
  "scripts": {
    "test": "export PATH=\"$PWD/.bin:$PATH\" && ls -al ./.bin && which refmt && which ocamlmerlin && which rebuild && which ocamlc && which ocamlopt && which berror",
    "postinstall": "./performActualInstallAndFixLinks.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reasonml/reason-cli.git"
  },
  "keywords": [
    "ReasonML",
    "Reason",
    "OCaml",
    "Merlin",
    "ocamlopt",
    "ocamlc",
    "refmt",
    "berror"
  ],
  "author": "Jordan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reasonml/reason-cli/issues"
  },
  "bin": {
    "ocamlmerlin-reason": "./.bin/ocamlmerlin-reason",
    "ocamlrun": "./.bin/ocamlrun",
    "ocamlc": "./.bin/ocamlc",
    "ocamlopt": "./.bin/ocamlopt",
    "ocaml": "./.bin/ocaml",
    "rtop": "./.bin/rtop",
    "utop": "./.bin/utop",
    "refmt": "./.bin/refmt",
    "refmttype": "./.bin/refmttype",
    "reopt": "./.bin/reopt",
    "rebuild": "./.bin/rebuild",
    "reactjs_jsx_ppx": "./.bin/reactjs_jsx_ppx",
    "berror": "./.bin/berror",
    "ocamlmerlin": "./.bin/ocamlmerlin"
  },
  "homepage": "https://github.com/reasonml/reason-cli#readme",
  "dependencies": {
    "esy": "git://github.com/reasonml/esy.git#beta-vef631e4"
  }
}
